<%= form_with model: @kitten do |form| %>
    
    <div>
        <%= form.label :name, "Name:" %>
        <%= form.text_field :name %>
    </div><br/>

    <div>
        <%= form.label :age, "Age:" %>
        <%= form.text_field :age %>
    </div><br/>

    <div>
        <%= form.label :cuteness, "Cuteness:" %>
        <%= form.text_field :cuteness %>
    </div><br/>

    <div>
        <%= form.label :softness, "Softness:" %>
        <%= form.text_field :softness %>
    </div><br/>

    <%= form.submit "Submit" %>

<% end %><br/>